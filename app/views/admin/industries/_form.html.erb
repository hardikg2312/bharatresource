<div class="bg-white pad15 font25">
  All industry
</div>
<div class="pad15">
  <div class="breadcrumb">
    Home / All industry / Add New industry
  </div>

  <div class="panel panel-white">
    <div class="panel-heading clearfix">
      <div class="panel-title">
        Add New industry
      </div>
    </div>
    <div class="panel-body clearfix">
      <div class="col-sm-6 col-md-6 col-lg-5">
        <div class="form-group">
          <%= f.text_field :name, :required => true, :class => "form-control", :placeholder=>"industry Name" %>
        </div>
        <div class="form-group">
          <%= f.submit "Save", :class => "btn btn-primary" %>
          <%= link_to admin_industries_path do %>
            <span class ="btn btn-warning">Cancel</span>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>